Subject: [PATCH] src: changed disk_cache save stats to disk interval

---
 net/disk_cache/blockfile/backend_impl.cc | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/net/disk_cache/blockfile/backend_impl.cc b/net/disk_cache/blockfile/backend_impl.cc
index 1608393b36c01..a142ac0ed3b88 100644
--- a/net/disk_cache/blockfile/backend_impl.cc
+++ b/net/disk_cache/blockfile/backend_impl.cc
@@ -1061,8 +1061,8 @@ void BackendImpl::OnStatsTimer() {
     }
   }
 
-  // Save stats to disk at 5 min intervals.
-  if (time % 10 == 0)
+  // Save stats to disk at 5*20 min intervals.
+  if (time % 200 == 0)
     StoreStats();
 }
 
